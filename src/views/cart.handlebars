<h1>Carrito:{{cart._id}}</h1>


<section class="product_container">
  {{#each cart.products}}
  <article>
    <h4>{{this.product.title}}</h4>
    <p>{{this.product.description}}</p>
    <p>precio: ${{this.product.price}}</p><!-- Agrega más propiedades del producto según tus necesidades -->
  </article>
  {{/each}}
  <button> confirmar compra</button>
</section>